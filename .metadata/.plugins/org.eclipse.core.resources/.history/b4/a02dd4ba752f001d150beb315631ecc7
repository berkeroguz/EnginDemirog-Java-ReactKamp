package asalSayilarDongu;

public class Main {

	public static void main(String[] args) {
		// 10 ile 100 arasındaki asal sayıları yazdıran program
		//1 veya kendinden başka böleni olmayan sayılara asal sayı denir.
		for (int sayi=10; sayi<100;sayi++)
		{
			for (int bol=1;bol<sayi;bol++)
			{
				if (sayi%bol==0 && sayi==bol)
				{
					
					System.out.println("Asal Sayılardan biri ="+ sayi);
					break;
						
				}
				bol++;
			}
			sayi++;
			
			
		}
		
		//Başla
		// Bir sayının asal olup olmadığını 2-3-4-5-6-7-8-9-20-22 bu sayı kendisine gelmeden önce bölünüyosa asal değildir.
		//sayi=10;
		//sayi<100 adim ... git
		//

	}

}
